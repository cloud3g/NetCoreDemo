@model List<Blog.Models.Resource>
@if (Model.Count() > 0)
{
    <div class="blog-module shadow">
        <div class="blog-module-title">最近分享</div>
        <ul class="fa-ul blog-module-ul">
            @foreach (var item in Model)
            {

                <li><i class="fa-li fa fa-hand-o-right"></i><a target="_blank" href="@item.Downloadlink" title="@item.Name">@item.Name</a></li>
            }
        </ul>
    </div>
}